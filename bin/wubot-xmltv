#!/usr/local/bin/perl -w
use strict;
use warnings;

use File::Temp qw/ tempdir /;
use Log::Log4perl;

use Wubot::Logger;
use Wubot::Util::XMLTV;

my $logger = Log::Log4perl::get_logger( 'default' );

my $tv = Wubot::Util::XMLTV->new();

my $tempdir = tempdir( "/tmp/tmpdir-XXXXXXXXXX", CLEANUP => 1 );

my $tmpfile = "$tempdir/tv-data.xml";

$tv->fetch_process_data( $tmpfile );


